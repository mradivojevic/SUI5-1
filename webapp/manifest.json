{
	"_version": "1.8.0",
	"sap.app": {
		"id": "mr.sapui.SUI5-1",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "mr.sapui.SUI5-1.view.View1",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "mr.sapui.SUI5-1.i18n.i18n"
				}
			},
			"MojModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": true,
				"uri": "model/MojModel.json"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "mr.sapui.SUI5-1.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "RouteView1",
				"pattern": "RouteView1",
				"target": ["TargetView1"]
			}, {
				"name": "View2",
				"pattern": "View2",
				"target": ["View2"]
			}, {
				"name": "View3",
				"pattern": "View3",
				"titleTarget": "",
				"greedy": false,
				"target": ["View3"]
			}],
			"targets": {
				"TargetView1": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "View1",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "mr.sapui.SUI5-1.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"viewLevel": 1
				},
				"View2": {
					"viewType": "XML",
					"viewPath": "mr.sapui.SUI5-1.view",
					"routerClass": "sap.m.routing.Router",
					"transition": "slide",
					"viewName": "View2",
					"viewLevel": 2,
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"async": true,
					"clearControlAggregation": false
				},
				"View3": {
					"viewType": "XML",
					"viewLevel": 2,
					"viewName": "View3"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}